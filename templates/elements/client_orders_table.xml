<div class="container-fluid" xmlns:t="http://twistedmatrix.com/ns/twisted.web.template/0.1" t:render="view">
    <div style="width: 100%;">
        <h1>Clients <a href="../?kind=promoter">Promoters</a></h1>
    </div>
    <div style="width: 100%;">
        <input type="hidden" name="kind">
            <t:attr name="value"><t:slot name="kind" /></t:attr>
        </input>
        <h2>
            Earn by Promoting These Tweets for Our Clients 
        </h2>
        <table style="width: 100%">
            <thead>
                <tr>
                    <th>Niche</th>
                    <th>Twitter Name</th>
                    <th>Status</th>
                    <th>Campaign Type</th>
                    <th>Retweet Target</th>
                    <th>Pay per Retweet</th>
                    <th>Actions</th>
                </tr>
            </thead>
            <tbody class="asks">
            </tbody>
        </table>
    </div>
    <div style="width: 100%;">
        If you are seeking to promote your tweets <a><t:attr name="href"><t:slot name="url" /></t:attr>Post Your Ask</a>
    </div>
</div>
